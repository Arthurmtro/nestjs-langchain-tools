{
  "name": "nestjs-langchain-tools",
  "version": "0.1.1",
  "description": "NestJS integration for LangChain tools with multi-agent support",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "typecheck": "tsc --noEmit",
    "typecheck:all": "tsc -p tsconfig.json --noEmit",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "test": "jest",
    "test:unit": "jest \"src/.*\\.spec\\.ts$\" --testPathIgnorePatterns=\"test/integration\"",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:integration": "jest test/integration --detectOpenHandles",
    "example": "sh scripts/run-example.sh",
    "example:build": "npm run build && npm run example",
    "prepublishOnly": "npm run build",
    "version:bump": "node scripts/version-and-publish.js",
    "ci:publish": "pnpm publish --no-git-checks"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arthurmonteiro/nestjs-langchain-tools.git"
  },
  "bugs": {
    "url": "https://github.com/arthurmonteiro/nestjs-langchain-tools/issues"
  },
  "homepage": "https://github.com/arthurmonteiro/nestjs-langchain-tools#readme",
  "keywords": [
    "nestjs",
    "langchain",
    "ai",
    "agents",
    "llm",
    "tools",
    "openai",
    "anthropic",
    "chatgpt",
    "claude",
    "gpt",
    "mistral",
    "api",
    "multi-agent"
  ],
  "author": "Arthur Monteiro",
  "license": "MIT",
  "peerDependencies": {
    "@langchain/anthropic": "^0.3.15",
    "@langchain/community": "^0.3.37",
    "@langchain/core": "^0.3.43",
    "@langchain/mistralai": "^0.2.0",
    "@langchain/ollama": "^0.2.0",
    "@langchain/openai": "^0.5.0",
    "@nestjs/common": "^11.0.12",
    "@nestjs/core": "^11.0.12",
    "langchain": "^0.3.19",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@nestjs/platform-express": "^11.0.12",
    "@nestjs/serve-static": "^5.0.3",
    "@nestjs/testing": "^11.0.12",
    "@types/express": "^5.0.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.14",
    "dotenv": "^16.4.7",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^29.7.0",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.3.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2",
    "typescript-eslint": "^7.12.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@nestjs/core"
    ]
  }
}